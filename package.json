{
  "name": "@adityahegde/typescript-framework",
  "version": "0.0.3",
  "description": "A framework for a full stack ui in expressjs and react",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc -p tsconfig-build.json",
    "test": "npm run test-mocha && npm run test-jest",
    "test-mocha": "nyc mocha test/unit/**/*.spec.ts test/functional/**/*.test.ts",
    "test-jest": "jest",
    "docs": "typedoc --out docs src/index.ts src/*/index.ts src/*/*/index.ts src/*/*/*/index.ts"
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "keywords": [
    "typescript",
    "decorators",
    "framwork",
    "fullstack",
    "server",
    "express",
    "authentication",
    "jwt",
    "session",
    "datastore",
    "mongodb",
    "mongoose",
    "reactjs",
    "antd"
  ],
  "mocha": {
    "ui": "bdd",
    "require": [
      "ts-node/register"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/unit/**/*.test.tsx"
    ],
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./test/unit/ui/setupTests.ts"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "exclude": [
      "src/ui/**/*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "lines": 75,
    "statements": 75
  },
  "author": "adityahegderocks@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@adityahegde/misc-scripts": "^1.0.1",
    "@adityahegde/typescript-test-utils": "^1.1.1",
    "@ant-design/icons": "^4.7.0",
    "@testing-library/jest-dom": "^5.15.0",
    "@testing-library/react": "^12.1.2",
    "@types/jest": "^27.0.2",
    "@types/mocha": "^9.0.0",
    "@types/node": "^16.11.7",
    "@types/react": "^17.0.35",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.4.0",
    "@typescript-eslint/parser": "^5.4.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "antd": "^4.16.13",
    "bcrypt": "^5.0.1",
    "clean-webpack-plugin": "^4.0.0",
    "connect-mongo": "^4.6.0",
    "css-loader": "^6.5.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.2.0",
    "express-session": "^1.17.2",
    "got": "^11.8.3",
    "html-webpack-plugin": "^5.5.0",
    "jest": "^27.3.1",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^18.1.0",
    "jsonwebtoken": "^8.5.1",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "less-vars-to-js": "^1.3.0",
    "mocha": "^9.1.3",
    "mongoose": "^6.0.13",
    "nyc": "^15.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^6.0.2",
    "react-test-renderer": "^17.0.2",
    "should": "^13.2.3",
    "sinon": "^12.0.1",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.3.1",
    "tough-cookie": "^4.0.0",
    "ts-jest": "^27.0.7",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "typedoc": "^0.22.9",
    "typedoc-plugin-markdown": "^3.11.6",
    "typescript": "^4.4.4",
    "webpack": "^5.64.1",
    "webpack-cli": "^4.9.1"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.6",
    "express": "^4.17.1",
    "pluralize": "^8.0.0",
    "validator": "^13.7.0",
    "winston": "^3.3.3"
  }
}
