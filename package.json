{
  "name": "typescript-framework",
  "version": "0.0.1",
  "description": "A framework for a full stack ui in expressjs and react",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf dist/ && npx tsc -p tsconfig-build.json",
    "test": "npm run test-mocha && npx jest",
    "test-mocha": "npx nyc npx mocha test/unit/**/*.spec.ts test/functional/**/*.test.ts"
  },
  "mocha": {
    "ui": "bdd",
    "require": [
      "ts-node/register"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*.jest.ts",
      "<rootDir>/test/**/*.test.tsx"
    ],
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "lines": 80,
    "statements": 80
  },
  "author": "adityahegderocks@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@ant-design/icons": "^4.6.2",
    "@types/jest": "^26.0.23",
    "@types/mocha": "^8.2.2",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.6",
    "@types/react-dom": "^17.0.5",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.1",
    "antd": "^4.15.6",
    "bcrypt": "^5.0.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "connect-mongo": "^4.4.1",
    "css-loader": "^5.2.5",
    "enzyme": "^3.11.0",
    "eslint": "^7.27.0",
    "express-session": "^1.17.2",
    "got": "^11.8.2",
    "html-webpack-plugin": "^5.3.1",
    "jest": "^26.6.3",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^16.5.3",
    "jsonwebtoken": "^8.5.1",
    "less": "^4.1.1",
    "less-loader": "^9.0.0",
    "less-vars-to-js": "^1.3.0",
    "mocha": "^8.4.0",
    "mongoose": "^5.12.10",
    "nyc": "^15.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "react-test-renderer": "^17.0.2",
    "should": "^13.2.3",
    "sinon": "^10.0.0",
    "source-map-loader": "^3.0.0",
    "style-loader": "^2.0.0",
    "tough-cookie": "^4.0.0",
    "ts-jest": "^26.5.6",
    "ts-loader": "^9.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.4",
    "webpack": "^5.37.1",
    "webpack-cli": "^4.7.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "pluralize": "^8.0.0",
    "validator": "^13.6.0",
    "winston": "^3.3.3"
  }
}
