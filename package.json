{
  "name": "@adityahegde/typescript-framework",
  "version": "0.0.1",
  "description": "A framework for a full stack ui in expressjs and react",
  "main": "dist/index.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "build": "rm -rf dist/ && tsc -p tsconfig-build.json",
    "test": "npm run test-mocha && jest",
    "test-mocha": "nyc mocha test/unit/**/*.spec.ts test/functional/**/*.test.ts",
    "docs": "typedoc --out docs src/index.ts src/*/index.ts src/*/*/index.ts src/*/*/*/index.ts"
  },
  "files": [
    "dist",
    "package.json",
    "package-lock.json",
    "README.md"
  ],
  "keywords": [
    "typescript",
    "decorators",
    "framwork",
    "fullstack",
    "server",
    "express",
    "authentication",
    "jwt",
    "session",
    "datastore",
    "mongodb",
    "mongoose",
    "reactjs",
    "antd"
  ],
  "mocha": {
    "ui": "bdd",
    "require": [
      "ts-node/register"
    ]
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/**/*.jest.ts",
      "<rootDir>/test/**/*.test.tsx"
    ],
    "preset": "ts-jest",
    "setupFilesAfterEnv": [
      "./node_modules/jest-enzyme/lib/index.js"
    ]
  },
  "nyc": {
    "check-coverage": true,
    "all": true,
    "extension": [
      ".ts"
    ],
    "include": [
      "src/**/*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text-summary"
    ],
    "lines": 80,
    "statements": 80
  },
  "author": "adityahegderocks@gmail.com",
  "license": "ISC",
  "devDependencies": {
    "@ant-design/icons": "^4.6.4",
    "@types/jest": "^27.0.1",
    "@types/mocha": "^9.0.0",
    "@types/node": "^15.6.0",
    "@types/react": "^17.0.19",
    "@types/react-dom": "^17.0.9",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.3",
    "antd": "^4.16.13",
    "bcrypt": "^5.0.1",
    "clean-webpack-plugin": "^4.0.0-alpha.0",
    "connect-mongo": "^4.5.0",
    "css-loader": "^6.2.0",
    "enzyme": "^3.11.0",
    "eslint": "^7.32.0",
    "express-session": "^1.17.2",
    "got": "^11.8.2",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.1.0",
    "jest-enzyme": "^7.1.2",
    "jsdom": "^17.0.0",
    "jsonwebtoken": "^8.5.1",
    "less": "^4.1.1",
    "less-loader": "^10.0.1",
    "less-vars-to-js": "^1.3.0",
    "mocha": "^9.1.0",
    "mongoose": "^5.13.8",
    "nyc": "^15.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.1",
    "react-test-renderer": "^17.0.2",
    "should": "^13.2.3",
    "sinon": "^11.1.2",
    "source-map-loader": "^3.0.0",
    "style-loader": "^3.2.1",
    "tough-cookie": "^4.0.0",
    "ts-jest": "^27.0.5",
    "ts-loader": "^9.2.5",
    "ts-node": "^10.2.1",
    "typedoc": "^0.21.7",
    "typedoc-plugin-markdown": "^3.10.4",
    "typescript": "^4.4.2",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cookie-parser": "^1.4.5",
    "express": "^4.17.1",
    "pluralize": "^8.0.0",
    "validator": "^13.6.0",
    "winston": "^3.3.3"
  }
}
